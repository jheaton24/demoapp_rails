source 'https://rubygems.org'

gem 'rails'
gem 'pg'

# Gems used only for assets and not required
# in production environments by default
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'

  gem 'uglifier'
end

gem 'jquery-rails'
gem 'bootsnap'
gem 'tzinfo-data'
gem 'webpacker', github: 'rails/webpacker'
gem 'yarn'
gem 'node.js'
